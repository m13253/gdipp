    <LinkerAdditionalOptions>-ext WixUIExtension</LinkerAdditionalOptions>
    <SuppressValidation>True</SuppressValidation>
    <Cultures>zh-cn</Cultures>
    <DefineConstants>codepage=936</DefineConstants>
  </PropertyGroup>
  <PropertyGroup>
    <PreBuildEvent>
