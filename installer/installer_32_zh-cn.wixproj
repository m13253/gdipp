    <PostBuildEvent>"$(WIX)\bin\torch.exe" -p -t language en-us\$(TargetFileName) $(Cultures)\$(TargetFileName) -out $(Cultures).mst
"$(WIX)\bin\EmbedTransform.exe" en-us\$(TargetFileName) $(Cultures).mst
del $(Cultures).mst</PostBuildEvent>
